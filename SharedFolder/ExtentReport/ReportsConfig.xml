<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
  <configuration>
    <!-- report theme -->
    <!-- standard, dark -->
    <theme>standard</theme>
  
    <!-- document encoding -->
    <!-- defaults to UTF-8 -->
    <encoding>UTF-8</encoding>
    
    <!-- protocol for script and stylesheets -->
    <!-- defaults to https -->
    <protocol>https</protocol>
    
    <!-- title of the document -->
    <documentTitle>TestReports</documentTitle>
    
    <!-- report name - displayed at top-nav -->
    <reportName>Shared Folder</reportName>
    
    <!-- report headline - displayed at top-nav, after reportHeadline -->
    <reportHeadline></reportHeadline>
    
    <!-- global date format override -->
    <!-- defaults to yyyy-MM-dd -->
    <dateFormat>yyyy-MM-dd</dateFormat>
    
    <!-- global time format override -->
    <!-- defaults to HH:mm:ss -->
    <timeFormat>HH:mm:ss</timeFormat>
    
    <!-- custom javascript -->
    <scripts>
      <![CDATA[
        $(document).ready(function() {
          
        });
      ]]>
    </scripts>
    
    <!-- custom styles -->
    <styles>
      <![CDATA[
     <!--    $(document).ready(function() {$('.dashboard-view').click();});
              $('.col.s12:nth-child(2) .card-panel.nm-v').hide();
              $('.col.s2:nth-child(2)').hide();
              $('.col.s12:nth-child(1)').css('width', '100%');
              $('.row .col.s2').css('width', '25%');  -->

      ]]>
    </styles>
  </configuration>
</extentreports>